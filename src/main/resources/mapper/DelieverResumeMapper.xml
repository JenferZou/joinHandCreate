<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.atxbai.online.mapper.DelieverResumeMapper">
<update id="setMark">
    UPDATE delieverresume SET mark = #{i} WHERE pid = #{id}
</update>
    <select id="selectSno" resultType="String">
        SELECT sno FROM delieverresume WHERE pid = #{id}
    </select>
</mapper>